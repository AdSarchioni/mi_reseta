<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Bootstrap demo</title>

  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">

  <style>
    /* CSS personalizado para reducir el padding y margen */
    .table-sm td,
    .table-sm th {
      padding: 0.1rem;
      /* Reduce el padding aún más */
      white-space: nowrap;
      /* Evita que el texto se ajuste en varias líneas */
      text-align: center;
      /* Ajusta el texto a la izquierda */
    }

    .table-container {
      margin-bottom: 0.5rem;
      /* Reduce el margen inferior entre tablas */
    }

    /* CSS personalizado para reducir el padding y el margen */
    .form-control {
      padding: 0.25rem;
      /* Reduce el padding dentro de los campos de texto */
    }

    .btn {
      padding: 0.25rem 0.5rem;
      /* Reduce el padding del botón */
    }

    .row.g-4 {
      margin-top: 0.5rem;
      /* Reduce el margen superior del grupo de filas */
      margin-bottom: 0.5rem;
      /* Reduce el margen inferior del grupo de filas */
    }

    .col-sm {
      padding: 0.25rem;
      /* Reduce el padding de las columnas */
    }
  </style>





</head>

<body>
  <h1>Crear Reseta</h1>


  <form action="/reseta_save" method="post">


    <div class="container">
      <div class="accordion" id="accordionExample">
        <div class="accordion-item">
          <h2 class="accordion-header">
            <button class="accordion-button collapsed " type="button" data-bs-toggle="collapse"
              data-bs-target="#collapseOne" aria-expanded="false" aria-controls="collapseOne" id="accordionButton1">
              Buscar Profesional
            </button>
          </h2>
          <div id="collapseOne" class="accordion-collapse collapse " data-bs-parent="">
            <div class="accordion-body">
              <div class="container">

                <div class="row g-4">
                  <div class="col-sm">
                    <input type="text" class="form-control" placeholder="Nombre" aria-label="Nombre" name="nombre_prof"
                      id="nombre_prof">
                  </div>
                  <div class="col-sm">
                    <input type="text" class="form-control" placeholder="Apellido" aria-label="Apellido"
                      name="apellido_prof" id="apellido_prof">
                  </div>
                  <div class="col-sm">
                    <input type="text" class="form-control" placeholder="Dni" aria-label="Dni" name="dni_prof"
                      id="dni_prof">

                  </div>


                  <div class="col-sm">
                    <button onclick="cargarProf()" type="button" class="btn btn-success w-100 py-2">Buscar
                      Profesional</button>
                  </div>
                </div>



                <div>
                  <table id="resultadosTabla1" class="table table-bordered table-dark table-sm table-hover">
                    <thead>
                      <tr class="bg-primary text-white">
                        <th scope="col">SEL</th>
                        <th scope="col">ID</th>
                        <th scope="col">NOMBRE</th>
                        <th scope="col">APELLIDO</th>
                        <th scope="col">EDAD</th>
                        <th scope="col">DIRECCION</th>
                        <th scope="col">DNI</th>
                        <th scope="col">TELEFONO</th>
                        <th scope="col">ACC</th>
                      </tr>
                    </thead>
                    <tbody>

                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>








        <div class="accordion-item">
          <h2 class="accordion-header">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
              data-bs-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo" id="accordionButton">
              Buscar Pasiente
            </button>
          </h2>
          <div id="collapseTwo" class="accordion-collapse collapse" data-bs-parent="">
            <div class="accordion-body">
              <div class="container">
                <div class="row g-3">
                  <div class="col-sm">
                    <input type="text" class="form-control" placeholder="Nombre" aria-label="Nombre" name="nombre"
                      id="nombre">
                  </div>
                  <div class="col-sm">
                    <input type="text" class="form-control" placeholder="Apellido" aria-label="Apellido" name="apellido"
                      id="apellido">
                  </div>
                  <div class="col-sm">
                    <input type="text" class="form-control" placeholder="Dni" aria-label="Dni" name="dni" id="dni">
                  </div>
                  <div class="col-sm">
                    <button onclick="cargarPas()" type="button" class="btn btn-success w-100 py-2">Buscar
                      Pasciente</button>
                  </div>
                </div>
              </div>

              <div class="container">

                <div>
                  <table id="resultadosTabla"
                    class="table table-bordered table-dark table -striped text-center mt-4  table-hover">
                    <thead>
                      <tr class="bg-primary text-white">
                        <th scope="col">SEL</th>
                        <th scope="col">ID</th>
                        <th scope="col">NOMBRE</th>
                        <th scope="col">APELLIDO</th>
                        <th scope="col">EDAD</th>
                        <th scope="col">DIRECCION</th>
                        <th scope="col">DNI</th>
                        <th scope="col">TELEFONO</th>
                        <th scope="col">ACC</th>
                      </tr>
                    </thead>
                    <tbody>

                    </tbody>
                  </table>
                </div>
              </div>

            </div>
          </div>
        </div>





        <div class="accordion-item">
          <h2 class="accordion-header">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse"
              data-bs-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
              Administracion
            </button>
          </h2>
          <div id="collapseThree" class="accordion-collapse collapse" data-bs-parent="">
            <div class="accordion-body">
              <div class="row">
                <div class="col-md-6 table-container">
                  <!-- Tabla 1 -->
                  <div class="container">

                    <div>
                      <table id="resultadosTabla"
                        class="table table-bordered table-dark table -striped text-center mt-4  table-hover">
                        <thead>
                          <tr class="bg-primary text-white">
                            <th scope="col">SEL</th>
                            <th scope="col">ID</th>
                            <th scope="col">NOMBRE</th>
                            <th scope="col">APELLIDO</th>
                            <th scope="col">EDAD</th>
                            <th scope="col">DIRECCION</th>
                            <th scope="col">DNI</th>
                            <th scope="col">TELEFONO</th>
                            <th scope="col">ACC</th>
                          </tr>
                        </thead>
                        <tbody>

                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
                <div class="col-md-6 table-container">
                  <!-- Tabla 2 -->
                  <div class="container">

                    <div>
                      <table id="resultadosTabla"
                        class="table table-bordered table-dark table -striped text-center mt-4  table-hover">
                        <thead>
                          <tr class="bg-primary text-white">
                            <th scope="col">SEL</th>
                            <th scope="col">ID</th>
                            <th scope="col">NOMBRE</th>
                            <th scope="col">APELLIDO</th>
                            <th scope="col">EDAD</th>
                            <th scope="col">DIRECCION</th>
                            <th scope="col">DNI</th>
                            <th scope="col">TELEFONO</th>
                            <th scope="col">ACC</th>
                          </tr>
                        </thead>
                        <tbody>

                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>

            </div>
          </div>
        </div>
      </div>



    </div>



    <div class="container">
      <div class="accordion" id="accordionPanelsStayOpenExample">
        <div class="accordion-item">
          <h2 class="accordion-header">
            <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseOne" aria-expanded="true" aria-controls="panelsStayOpen-collapseOne">
              Prestacion
            </button>
          </h2>
          <div id="panelsStayOpen-collapseOne" class="accordion-collapse collapse show">
            <div class="accordion-body">
             
            </div>
          </div>
        </div>
        <div class="accordion-item">
          <h2 class="accordion-header">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseTwo" aria-expanded="false" aria-controls="panelsStayOpen-collapseTwo">
              Diagnostico
            </button>
          </h2>
          <div id="panelsStayOpen-collapseTwo" class="accordion-collapse collapse">
            <div class="accordion-body">
             
            </div>
          </div>
        </div>
        <div class="accordion-item">
          <h2 class="accordion-header">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseThree" aria-expanded="false" aria-controls="panelsStayOpen-collapseThree" id="accordionButtonMedicamentos">
              Buscar Medicamentos
            </button>
          </h2>
          <div id="panelsStayOpen-collapseThree" class="accordion-collapse collapse">
            <div class="accordion-body">
            
           
           
           
    
            
                    <div class="container">
                      <label for="cantidadMedicamentos" class="form-label">Cantidad de Medicamentos:</label>
                      <input type="number" class="form-control" id="cantidadMedicamentos" name="cantidadMedicamentos" min="1">
                    </div>
                    <div class="container" id="contenedorBusquedaMedicamentos">
                      <!-- Aquí se agregarán dinámicamente los campos de búsqueda de medicamentos -->
                    </div>
                    <button type="button" class="btn btn-primary mt-2" onclick="agregarCamposBusqueda()">Agregar campos de búsqueda</button>          
           
            </div>
          </div>
        </div>
      </div>

    </div>





    <div>
      <div class="container">
        <p class="d-inline-flex gap-1">
          <a href="" type="button" class="btn btn-info w-100 py-2">Reseta</a>
          <a href="/logout" type="button" class="btn btn-danger w-100 py-2">Logout</a>
        </p>
      </div>

    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>










  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
    crossorigin="anonymous"></script>

  <script src="./js/prueba.js"></script>

</body>

</html>