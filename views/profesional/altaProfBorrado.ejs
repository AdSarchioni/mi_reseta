<!doctype html>
<html lang="en" data-bs-theme="auto">

<head>
  <script src="../assets/js/color-modes.js"></script>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="">
  <style>
    .dt-center {
        text-align: center;
    }
    .inactive {
        opacity: 0.5; /* Reduce la opacidad para hacer la fila menos prominente */
    }

  </style>
  <title>Mi Reseta</title>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.0/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://cdn.datatables.net/2.0.8/css/dataTables.bootstrap5.css">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>



<body class="d-flex align-items-center py-60 bg-success-subtle ">
  <div></div>
  <main class="form-signin w-100 m-4">

    <div>
      <div class="card mb-3" style="max-width: 540px;">
        <div class="row g-0">
          <div class="col-md-4">
            <img src="./img/matrix.png" class="img-fluid rounded-start" alt="...">
          </div>
          <div class="col-md-8">
            <div class="card-body">
              <h5 class="card-title">Alta Profesional Borrado</h5>
              <p class="card-text"> </p>
              <p class="card-text"><small class="text-body-secondary">si tiene dudas puede buscarse primero</small></p>
            </div>
          </div>
        </div>
      </div>

    </div>

  



    
   
      <div class="col-12">
      
        <a type="button" href="/profesional" class="btn btn-warning">Volver</a>
        <a type="button" href="logout" class="btn btn-danger">Logout</a>
      </div>
  




    <div class="container">
      <table id="myTable" name="myTable" class="table table-striped" style="width:100%">

        <thead>
          <tr class="bg-primary text-white">
            <th scope="col">ID</th>
            <th scope="col">REFEPS</th>
            <th scope="col">ESPECIAL</th>
            <th scope="col">MAT</th>
            <th scope="col">NOMBRE</th>
            <th scope="col">APELLIDO</th>
            <th scope="col">DNI</th>
            <th scope="col">DOMIC</th>
            <th scope="col">EMAIL</th>
            <th scope="col">TEL</th>
            <th scope="col">ALTA</th>
            <th scope="col">DAR ALTA</th>
          
          </tr>
        </thead>
  
        <tbody>
  
        </tbody>
  
      </table>
    </div>








  </main>












  <script src="https://code.jquery.com/jquery-3.7.1.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.3.0/js/bootstrap.bundle.min.js"></script>
  <script src="https://cdn.datatables.net/2.0.8/js/dataTables.js"></script>
  <script src="https://cdn.datatables.net/2.0.8/js/dataTables.bootstrap5.js"></script>
  <script src="./js/altaProfBorrado.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
  <% if(alert){%>
    <script>
      Swal.fire({
        title: '<%= alertTitle %>',
        text: '<%= alertMessage %>',
        icon: '<%= alertIcon %>',
        showConfirmButton: '<%= showConfirmButton %>',
        timer: '<%= 3000 %>'
      }).then(() => {
        window.location = '/<%= ruta %>'
      })
    </script>
    <% } %>



</body>

</html>